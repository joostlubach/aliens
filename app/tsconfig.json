{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "experimentalDecorators": true,

    "typeRoots": [
      "node_modules",
      "typings",
    ],
    "types": [
      "yaml"
    ],

    "paths": {
      "~/*": ["./src/*"],
      "%*": ["./assets/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "../vendor/mobx-store/src/useStore.ts", "../vendor/mobx-store/src/StoreContext.tsx"  ]
}
